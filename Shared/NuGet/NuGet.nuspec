<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>Zebble.WebView</id>
    <version>4.2.236</version>
    <title>WebView for Zebble (Xamarin)</title>
    <description>Displays Html content and web pages for Zebble.</description>
    <projectUrl>https://github.com/Geeksltd/Zebble.WebView</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/Geeksltd/Zebble.WebView/master/Shared/Icon.png</iconUrl>
    <authors>Mobile App Technologies Ltd</authors>
    <owners>Mobile App Technologies Ltd</owners>
    <copyright>Copyright ©2017 | Mobile App Technologies Ltd</copyright>
    <dependencies>
      <group targetFramework="UAP10">
        <dependency id="HtmlAgilityPack" version="1.4.9.5" />
      </group>
      <group targetFramework="MonoAndroid">
        <dependency id="HtmlAgilityPack" version="1.4.9.5" />
      </group>
      <group targetFramework="Xamarin.iOS10">
        <dependency id="HtmlAgilityPack" version="1.4.9.5" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="..\..\lib\Zebble.WebView.iOS.*" target="lib\Xamarin.iOS10\" />
    <file src="..\..\lib\Zebble.WebView.Android.*" target="lib\MonoAndroid10\" />
    <file src="..\..\lib\Zebble.WebView.UWP.*" target="lib\UAP10\" />
  </files>
</package>