<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>Zebble.Plugin.WebView</id>
    <version>4.1.214</version>
    <title>WebView for Zebble for Xamarin</title>
    <description>Displays Html content and web pages for Zebble for Xamarin.</description>
    <iconUrl>https://raw.githubusercontent.com/{MyUserName}/{MyRepo}/.../Shared/Icon.png</iconUrl>
    <authors>Mobile App Technologies Ltd</authors>
    <owners>Mobile App Technologies Ltd</owners>
    <copyright>Copyright Â©2017 | Mobile App Technologies Ltd</copyright>
    <projectUrl>https://github.com/Geeksltd/Zebble.WebView</projectUrl>

    <dependencies>
      <group targetFramework="UAP10">
        <dependency id="HtmlAgilityPack" version="1.4.9.5" />
      </group>
      <group targetFramework="MonoAndroid">
        <dependency id="Zebble" version= "4.0.232"/>
        <dependency id="HtmlAgilityPack" version="1.4.9.5" />
      </group>
      <group targetFramework="Xamarin.iOS10">
        <dependency id="Zebble" version= "4.0.232"/>
        <dependency id="HtmlAgilityPack" version="1.4.9.5" />
      </group>
    </dependencies>

  </metadata>
  <files>
    <file src="..\lib\Zebble.WebView.iOS.*" target="lib\Xamarin.iOS10\" />
    <file src="..\lib\Zebble.WebView.Android.*" target="lib\MonoAndroid10\" />
    <file src="..\lib\Zebble.WebView.UWP.*" target="lib\UAP10\" />
  </files>
</package>